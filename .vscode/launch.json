{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "setparms",
            "program": "${workspaceFolder}/dist/setparms.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "args": ["--predictionEndpoint https://us-documentai.googleapis.com/v1/projects/627140639739/locations/us/processors/843f90f0ef2bfca3:process"]
        },
        {
            "type": "node",
            "name": "process_doc",
            "program": "${workspaceFolder}/dist/process_doc.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "pwa-node config",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/process_doc.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}